<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WaterRippleView">
        <attr name="rippleSpacing" format="dimension|reference"/>
        <attr name="rippleCount" format="integer|reference"/>
        <attr name="rippleColor" format="color|reference"/>
        <attr name="rippleCenterIcon" format="reference"/>
        <attr name="rippleAutoRunning" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CircleLoadingView">
        <attr name="circleStartColor" format="color|reference"/>
        <attr name="circleEndColor" format="color|reference"/>
        <attr name="circleArcWidth" format="integer|reference"/>
        <attr name="circleArcRadian" format="integer|reference"/>
        <attr name="circleArcSpacing" format="integer|reference"/>
    </declare-styleable>

</resources>